{
    "manifest_version": 2,
    "name": "Recipe Jumper",
    "version": "1.0",
    "description": "we aint got time for your life story...lets jump to the recipe already!",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "exclude_globs": [
              "*://www.google.com/*"],
            "js": ["jquery.min.js", "./main.js"],
            "css": ["index.css"]
        }
    ]     
}



